			function add_notes(day, columns, add){			
			        var tf = ['true','false'];					
					tf.forEach(function(entry) {
								var id= '#' +entry + '-' + day;
								for (var key in columns) {
											var value = columns[key];
											//var color = '#true-2-1-friday td.tdn_' + key;
											var color = id + ' td.tdn_' + key;
											$(color).addClass(value);
								}									
								$(id).after(notes);	
								$(id).next('div').addClass(day);
					});					
					var classx = '.' + day + ' .t-table-notes';	
					 for (var key in add) {
						var value = add[key];
						$(classx).append(app[value]);
					}	
			}
			
