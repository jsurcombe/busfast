            <script>
      function initialize11() {
        var myLatlng = new google.maps.LatLng(49.460667, -2.587168);
        var mapOptions = {
          zoom: 11,
          center: myLatlng,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        }

        var map = new google.maps.Map(document.getElementById('map_canvas11'), mapOptions);
		
			var myKmlOptions = {
						preserveViewport: true
					}

        var georssLayer = new google.maps.KmlLayer('http://buses.gg/kmlx/11.kml',myKmlOptions);
		        georssLayer.setMap(map);
		georssLayer2.setMap(map);
      }
    </script> 
             
